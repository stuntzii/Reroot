{
  "dist": "./dist",
  "source": "./src",
  "releases": "./",
  "release_name": "release",
  "manifest": "./src/manifest.json",
  "js": "./src/**/*.js",
  "js_bundles": [
    {
      "src": "./src/inject/index.js",
      "name": "inject"
    }
  ],
  "html": "./src/**/*.html",
  "scss": "./src/**/*.scss",
