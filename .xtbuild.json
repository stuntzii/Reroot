  "assets": ["./assets/**/*", "!./assets/locales", "!./assets/locales/**/*"],
  "copyAsIs": [],
  "locales_dir": "./assets/locales/",
  "locales_list": ["en"],
  "commands": null,
  "commands_watch_path": null
}
