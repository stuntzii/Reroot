{
  "name": "Reroot",
  "description": "Extension for twitter that gives you the option to also post to lens protocol",
  "version": "1.1",
  "manifest_version": 3,
  "action": {
    "default_icon": {
      "16": "assets/icon-16.png",
      "32": "assets/icon-32.png",
      "48": "assets/icon-48.png",
      "128": "assets/icon-128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["https://twitter.com/*"],
      "js": ["inject.js"]
    }
  ],
  "icons": {
    "16": "assets/icon-16.png",
    "32": "assets/icon-32.png",
    "48": "assets/icon-48.png",
    "128": "assets/icon-128.png"
  }
}
